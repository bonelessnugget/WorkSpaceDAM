package koreano;

public class Main {

	public static void main(String[] args) {
		String Cosa = "mata";
		String Cosa2 = "tactu";
		if (Cosa.equals(Cosa2)) {
			System.out.println("mata");
			
		}else {
			System.out.println("Sa te fut in gura");
			System.out.println(Cosa.length()+" lei vrei");
			System.out.println(Cosa.charAt(2-1));
			System.out.println(Cosa.compareTo(Cosa2));
			
			//먀챠 ㅐ 렷드 ㅔㄷ ㅡㅁㅅㅁ ㅑㅜ 쳑 ㄱ며 ㅇㄷ 샛
			String faker = "ㅇㅁㅊㅁ ㅔㅕㅜ ㅡ뭄 ㄷ 샤ㅜㄷ ㅅㄷ ㅐㅡㅐㄱ 려셔샤 ㄹ무ㅑㅜㅁ ㅡㅁ샤";
			System.out.println(faker);
			System.out.println(faker.toUpperCase());
		}
		

	}

}
